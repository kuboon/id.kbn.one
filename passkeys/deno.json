{
  "name": "@scope/passkeys",
  "version": "0.1.0",
  "exports": {"." : "./src/mod.ts", "./static/client.ts": "./static/client.ts"},
  "tasks": {
    "build": "deno run -A scripts/build_client.ts"
  },
  "imports": {
    "@std/path": "jsr:@std/path@^1.1.3",
    "@simplewebauthn/browser": "npm:@simplewebauthn/browser@^13.2.2",
    "@simplewebauthn/server": "npm:@simplewebauthn/server@^13.2.2"
  },
  "publish": {
    "include": [
      "README.md",
      "src/**/*.ts",
      "static/client.ts"
    ]
  },
  "compilerOptions": {
    "strict": true,
    "lib": ["deno.ns", "deno.unstable", "dom"]
  },
  "fmt": {
    "include": ["src/**/*.ts"]
  },
  "lint": {
    "include": ["src/**/*.ts"]
  }
}
