{
  "imports": {
    "hono": "npm:hono@4.10.1",
    "@kuboon/hono-passkeys-middleware": "../hono-middleware/mod.ts",
    "@hexagon/base64": "npm:@hexagon/base64@2.0.4"
  },
  "tasks": {
    "serve": "deno serve -P deno-server.ts",
    "dev": "deno serve -P --watch deno-server.ts"
  },
  "permissions": {
    "default": {
      "env": ["RP_ID", "RP_NAME", "IDP_ORIGIN", "ORIGINS"],
      "net": true,
      "read": [".."]
    }
  },
  "compilerOptions": {
    "lib": ["deno.ns", "deno.unstable"]
  }
}
