{
  "imports": {
    "@scope/hono-passkeys-middleware": "../hono-middleware/src/mod.ts",
    "@negrel/webpush": "jsr:@negrel/webpush@0.5.0"
  },
  "tasks": {
    "serve": "deno serve -P deno-server.ts",
    "dev": "deno serve -P --allow-net --env --watch deno-server.ts"
  },
  "permissions": {
    "default": {
      "env": [
        "RP_ID",
        "RP_NAME",
        "IDP_ORIGIN",
        "ORIGINS",
        "PUSH_CONTACT",
        "HMAC_KEY"
      ],
      "net": true,
      "read": [".."]
    }
  },
  "compilerOptions": {
    "lib": ["deno.ns", "deno.unstable", "dom"]
  }
}
